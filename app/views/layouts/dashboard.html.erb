<!DOCTYPE html>
<html>
<head>
  <title>Thalarion Panel</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>

  <link href="//fonts.googleapis.com/css?family=Roboto+Light:200,400,600" rel="stylesheet" type="text/css">

  <%= csrf_meta_tags %>
</head>
<body>
  <div class="dashboard-container">
    <div class="container">
      <div class="row">
        <div class=".hidden-xs col-md-3">
          <%= render 'partials/navigation' %>
        </div>
        <div class="col-xs-12 col-md-9">
          <div class="row">
            <div class="col-xs-10">
              <div class="title-main">Dashboard</div>
              <div class="title-section">Overview</div>
            </div>
            <div class="col-xs-2">
              User
            </div>
          </div>
          <hr>
          <%= yield %>
        </div>
      </div>
    </div>
  </div>
</body>
</html>
